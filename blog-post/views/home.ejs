<%- include("partials/header") -%>
<div class="container">
    <h1>Home</h1>
    <p><%= startingContent %></p>
    <% posts.forEach(function(object) { %>
        <h3><%= object.title %></h3>
        <p><%= object.content.substring(0,100)+"..." %><a href="/post/<%=object._id%>">click here</a></p>

    <% }); %>
</div>
